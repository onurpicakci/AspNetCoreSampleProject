@{
    Layout = null;
}

<!DOCTYPE html>
<html>

@await Html.PartialAsync("/Views/Admin/PartialHeader.cshtml")
<body>
<nav class="sidenav navbar navbar-vertical  fixed-left  navbar-expand-xs navbar-light bg-white" id="sidenav-main">
    <div class="scrollbar-inner">
        <div class="sidenav-header  align-items-center">
            <a class="navbar-brand" href="javascript:void(0)">
                <img src="~/argon-dashboard-master/assets/img/brand/blue.png" class="navbar-brand-img" alt="...">
            </a>
        </div>
        @await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")
    </div>
</nav>
<div class="main-content" id="panel">
    @await Html.PartialAsync("/Views/Admin/PartialNavBar.cshtml")
    @await Html.PartialAsync("/Views/Admin/NavigationPartial.cshtml")
    <div class="container-fluid mt--6">
        <div class="row">
            <div class="col">
                @RenderBody()
            </div>
        </div>
        <br/> <br/>
        @await Html.PartialAsync("/Views/Admin/PartialFooter.cshtml")
    </div>
</div>
@await Html.PartialAsync("/Views/Admin/PartialScripts.cshtml")
</body>

</html>